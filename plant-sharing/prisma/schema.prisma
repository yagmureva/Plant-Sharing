// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db" // Başlangıç için SQLite, istersen PostgreSQL de kullanabilirsin
}

model Plant {
  id        Int      @id @default(autoincrement())
  name      String
  imageUrl  String
  ownerMail String
  address   String
  createdAt DateTime @default(now())
}
